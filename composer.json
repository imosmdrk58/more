{
    "name": "jhin1m/mangacore",
    "description": "MangaCore - Laravel package for manga reading platform management. Complete refactor from OphimCore movie streaming CMS to manga content management system.",
    "type": "library",
    "keywords": ["laravel", "php", "manga", "cms", "content-management", "reading-platform", "backpack", "laravel-package", "manga-reader", "image-processing"],
    "homepage": "https://github.com/jhin1m/mangacore",
    "authors": [
        {
            "name": "jhin1m",
            "email": "your-email@example.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jhin1m/mangacore/issues",
        "source": "https://github.com/jhin1m/mangacore"
    },
    "require": {
        "laravel/framework": "^6|^7|^8",
        "ckfinder/ckfinder-laravel-package": "v3.5.2.1",
        "hacoidev/laravel-caching-model": "^1.0.0",
        "hacoidev/crud": "^1.0.0",
        "hacoidev/settings": "^1.0.0",
        "hacoidev/permissionmanager": "^1.0.0",
        "spatie/laravel-sitemap": "^5.8",
        "artesaos/seotools": "^v0.22.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ophim\\Core\\": "src/",
            "Ophim\\Core\\Database\\Factories\\": "database/factories/",
            "Ophim\\Core\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ophim\\Core\\OphimServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
